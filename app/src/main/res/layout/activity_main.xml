        app:layout_constraintTop_toBottomOf="@+id/btn_result6" />

    <Button
        android:id="@+id/btn_result6"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/btns"
        android:text="6"
        android:textColor="@color/white"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_result9"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.016"
        app:layout_constraintStart_toEndOf="@+id/btn_result5"
        app:layout_constraintTop_toBottomOf="@+id/btn_result3" />

    <Button
        android:id="@+id/btn_result7"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/btns"
        android:text="7"
        android:textColor="@color/white"
        android:textSize="48sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
