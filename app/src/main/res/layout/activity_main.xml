    <Button
        android:id="@+id/btn_result2"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:layout_marginStart="32dp"
        android:background="@drawable/btns"
        android:text="2"
        android:textColor="@color/white"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_result5"
        app:layout_constraintStart_toEndOf="@+id/btn_result1"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btn_result4"
        android:layout_width="80dp"
        android:layout_height="80dp"
        android:background="@drawable/btns"
        android:text="4"
        android:textColor="@color/white"
        android:textSize="48sp"
        app:layout_constraintBottom_toTopOf="@+id/btn_result7"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.086"
        app:layout_constraintStart_toEndOf="@+id/btn_force_train"
        app:layout_constraintTop_toBottomOf="@+id/btn_result1" />

    <Button
        android:id="@+id/btn_llap_stop"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="llap stop"
        android:visibility="gone"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.347"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.217"
